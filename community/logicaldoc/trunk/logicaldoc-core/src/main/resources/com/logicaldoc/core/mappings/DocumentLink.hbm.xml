<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.logicaldoc.core.document.DocumentLink" table="ld_link">
		<cache usage="read-write" />
        <id name="id" type="long" column="ld_id" unsaved-value="0">
            <generator class="increment" />
        </id>
		<property name="docId1" type="long" column="ld_docid1" not-null="true" />
		<property name="docId2" type="long" column="ld_docid2" not-null="true" />
		<property name="type" type="string" column="ld_type" length="255" />
	</class>
</hibernate-mapping>