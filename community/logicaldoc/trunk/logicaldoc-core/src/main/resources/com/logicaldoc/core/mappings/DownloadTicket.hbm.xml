<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.logicaldoc.core.document.DownloadTicket" table="ld_ticket">
		<cache usage="read-write" />
        <id name="id" type="long" column="ld_id" unsaved-value="0">
            <generator class="increment" />
        </id>
		<property name="ticketId" type="string" column="ld_ticketid" />
		<property name="docId" type="long" column="ld_docid" />
		<property name="username" type="string" column="ld_username" length="30" />
	</class>
</hibernate-mapping>